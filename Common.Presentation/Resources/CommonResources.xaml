<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:convers="clr-namespace:Common.Presentation.Converters">

    <ResourceDictionary.MergedDictionaries>
        <!--<ResourceDictionary Source="WPF Themes/BureauBlue.xaml"/>-->
        <ResourceDictionary Source="ImageResources.xaml"/>
        <ResourceDictionary Source="ThemeResources.xaml"/>
        <ResourceDictionary Source="ColorResources.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    <convers:StrToBitmapImageConverter x:Key="StrToImageConver"/>
    <convers:StrToIntConverter x:Key="StrToIntConver"/>
    <convers:StrToUriConverter x:Key="StrToUriConver"/>
    <convers:StrToDoubleConverter x:Key="StrToDoubleConver"/>
    <convers:BoolReversalConverter x:Key="BoolReversalConver"/>
    <convers:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConver"/>
    <convers:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverReversal" Reversal="True"/>
</ResourceDictionary>